.b-contacts
  margin 0 0 0 26px

.b-contacts__inner
  padding 0 0 17px
  position relative
  background #fff
  border-radius 4px
  text-align center

.b-contacts__person
  position absolute
  top -23px
  left 50%
  margin 0 0 0 -25px
  width 50px
  height 50px
  background #eaeaea
  border-radius 50%

.b-contacts__person-photo
  position absolute
  top -4px
  left -1px
  border-radius 50%

.b-contacts__head
  padding 33px 5px 14px
  border-bottom 1px solid #f3f3f3
  font-size 13px

.b-contacts__title
  display block
  margin 0 0 6px
  font-size 13px
  color #454545
  line-height 17px

.b-contacts__text
  margin 0

.b-contacts__list
  margin 14px 16px 0
  padding 0
  list-style none

.b-contacts__item
  margin 10px 0 0
  height 15px
  line-height 23px
  background #f0f0f0
  border 1px dashed #e3e3e3
  text-align center
  &:first-child
    margin 0

.b-contacts__icon,
.b-contacts__stretch
  display inline-block
  vertical-align

.b-contacts__icon
  width 20px
  color #a1a1a1
  font-size 17px
  text-align left

.b-contacts__stretch
  position relative
  width 127px
  height 15px
  color #d0d0d0
  font-style normal
  overflow hidden

.b-contacts-num
  position absolute
  top -1px

.b-contacts__info
  margin 4px 0 0
  display block
  animation tada linear 1s
  animation-iteration-count 1
  transform-origin 50% 50%
  -webkit-animation tada linear 1s
  -webkit-animation-iteration-count 1
  -webkit-transform-origin 50% 50%
  -moz-animation tada linear 1s
  -moz-animation-iteration-count 1
  -moz-transform-origin 50% 50%
  -o-animation tada linear 1s
  -o-animation-iteration-count 1
  -o-transform-origin 50% 50%
  -ms-animation tada linear 1s
  -ms-animation-iteration-count 1
  -ms-transform-origin 50% 50%

@-webkit-keyframes tada {
  0% {
    -webkit-transform:  rotate(0deg) scaleX(1.00) scaleY(1.00) ;
  }
  10% {
    -webkit-transform:  rotate(-3deg) scaleX(0.80) scaleY(0.80) ;
  }
  20% {
    -webkit-transform:  rotate(-3deg) scaleX(0.80) scaleY(0.80) ;
  }
  30% {
    -webkit-transform:  rotate(3deg) scaleX(1.20) scaleY(1.20) ;
  }
  40% {
    -webkit-transform:  rotate(-3deg) scaleX(1.20) scaleY(1.20) ;
  }
  50% {
    -webkit-transform:  rotate(3deg) scaleX(1.20) scaleY(1.20) ;
  }
  60% {
    -webkit-transform:  rotate(-3deg) scaleX(1.20) scaleY(1.20) ;
  }
  70% {
    -webkit-transform:  rotate(3deg) scaleX(1.20) scaleY(1.20) ;
  }
  80% {
    -webkit-transform:  rotate(-3deg) scaleX(1.20) scaleY(1.20) ;
  }
  90% {
    -webkit-transform:  rotate(3deg) scaleX(1.20) scaleY(1.20) ;
  }
  100% {
    -webkit-transform:  rotate(0deg) scaleX(1.20) scaleY(1.20) ;
  }
}

